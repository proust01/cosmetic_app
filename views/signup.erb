
<div class="signup-con">

  <h1 class="title">Sign Up</h1>

  <h3 class="info">Basic Info</h3>
  <form action="/signup" method="post">
      <label for="">email</label>
      <input class="email" type="text" name="email">

      <label for="">password</label>
      <input class="email" type="password" name="password">

  

  <h3 class="info">Skin Info</h3>

  <h5 class="skin">gender</h5>

  <select class="custom-select custom-select-lg mb-3">
    <option selected>Open this select menu</option>
    <option value="1">male</option>
    <option value="2">female</option>
  </select>

  <h5 class="skin">age</h5>
  <input class="email" type="number" name="age">


  <h5 class="skin">skin type</h5>
  <select class="custom-select custom-select-lg mb-3">
    <option selected>Open this select menu</option>
    <option value="1">dry</option>
    <option value="2">mild</option>
    <option value="3">oily</option>
    <option value="4">combination</option>
  </select>

  <h5 class="skin">skin trouble</h5>
  <select class="custom-select custom-select-lg mb-3">
    <option selected>Open this select menu</option>
    <option value="1">none</option>
    <option value="2">acne</option>
    <option value="3">oily</option>
    <option value="4">redness</option>
    <option value="5">uneven tone</option>
    <option value="6">sensitive</option>
    <option value="7">ageing</option>
  </select>

  <h5 class="skin"></h5>
    <button style="width: 200px height: 40px">Sign Up</button>
  </form>
</div>




